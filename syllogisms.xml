<fracas-problems>
  <problem id="1" fracas_answer="yes">
    <p idx="1">All men are animals.</p>
    <p idx="2">All animals are mortal.</p>
    <q>Are all men mortal?</q>
    <h></h>
    <note>Barbara</note>
  </problem>
